{
  "plugins": ["@prettier/plugin-pug"],
  "pugPrintWidth": 100,
  "pugBracketSpacing": false,
  "pugAttributeSeparator": "none",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
  "singleAttributePerLine": false
}
